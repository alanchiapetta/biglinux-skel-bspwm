# BSPWM aliases

alias e='exit'
alias q='exit'
alias q:='exit'
alias wm='xprop | grep WM_CLASS'
alias tempo='curl http://wttr.in'
alias cd..='cd ..'
alias ll='ls -lAhX --color'
alias l='ls -h --color'
alias la='ls -a --color'
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'
alias ......='cd ../../../../..'
alias .......='cd ../../../../../..'
alias pac='sudo pacman -Syu'
alias pacin='sudo pacman -S'
alias pacinfo='pacman -Si'
alias pacsearch='pacman -Ss'
alias bspconfig='nvim ~/.config/bspwm/bspwmrc'
alias keyconfig='nvim ~/.config/bspwm/sxhkdrc'
alias barconfig='nvim ~/.config/bspwm/polybar/config'
alias rangerconfig='nvim ~/.config/ranger/rc.conf'
alias bashconfig='nvim ~/.bashrc'
alias zshconfig='nvim ~/.zshrc'
alias aliasconfig='nvim ~/.bash_aliases'
alias clone='git clone'
alias neo='neofetch'
alias so='source ~/.bashrc'
alias soz='source ~/.zshrc'
alias bspfolder='cd ~/.config/bspwm/'
